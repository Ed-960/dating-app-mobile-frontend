{
  "name": "dating-frontend",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "BACKEND_URL=http://localhost:3000 expo start",
    "start:prod": "expo start --no-dev --minify",
    "android": "expo run:android",
    "android:dev": "BACKEND_URL=http://localhost:3000 expo run:android",
    "android:prod": "BACKEND_URL=https://your-backend-url.com expo run:android --release",
    "ios": "expo run:ios",
    "ios:dev": "BACKEND_URL=http://localhost:3000 expo run:ios",
    "ios:prod": "BACKEND_URL=https://your-backend-url.com expo run:ios --configuration Release",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "dependencies": {
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@tanstack/react-query": "^5.59.0",
    "dayjs": "^1.11.13",
    "@expo/vector-icons": "~14.0.4",
    "expo": "~52.0.0",
    "expo-asset": "~11.0.5",
    "expo-auth-session": "~6.0.0",
    "expo-build-properties": "~0.12.0",
    "expo-constants": "~17.0.0",
    "expo-crypto": "~14.0.0",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.0",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-web-browser": "~14.0.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.6.0"
  }
}
